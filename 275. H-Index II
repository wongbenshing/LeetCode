class Solution:
    def hIndex(self, citations):
        N = len(citations)
        l, h = 0, N-1
        while l <= h:
            mid = (l + h) // 2
            if (N - mid) > citations[mid]:
                l = mid + 1
            else:
                h = mid - 1
        return N - l
        """
        :type citations: List[int]
        :rtype: int
        """
        
